import smtplib, ssl
port = 465
context = ssl.create_default_context()
password ="password"

messages=["""""\
Subject: Test1

testing124""",

          """""\
Subject: Test2

www.google.com""",
          
          """""\
Subject: Test3

www.google.com/pickle""",
          ]



with smtplib.SMTP_SSL("smtp.gmail.com", port, context=context) as server:
    server.login("your@email.com", password)
    for x in messages:
        server.sendmail("your@email.com", "recepiant@email.com", x)
